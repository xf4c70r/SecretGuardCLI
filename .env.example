# Environment variables for SecretGuard Evaluation
# Copy this file to .env and fill in your values

# HuggingFace Configuration
HF_TOKEN=your_huggingface_token_here
HF_REPO=asudarshan/secretguard-starcoderbase-lora2
BASE_MODEL=bigcode/starcoderbase

# Model Configuration
USE_QUANTIZATION=true
MAX_LENGTH=512
MAX_NEW_TOKENS=10
TEMPERATURE=0.0

# Evaluation Configuration
BATCH_SIZE=1
SAVE_DETAILED_RESULTS=true
GENERATE_PLOTS=true

# Performance Optimization (for M3 Mac)
PYTORCH_ENABLE_MPS_FALLBACK=1
TOKENIZERS_PARALLELISM=false

# Logging
LOG_LEVEL=INFO
LOG_FILE=evaluation.log

# Output Configuration
OUTPUT_DIR=data/evaluation_results
TEST_DATA_PATH=data/test_samples.json